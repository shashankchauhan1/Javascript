// printinf by using for loop.

let arr=['apple','mango','banana','orange'];
let len=arr.length;
for(let i=0; i<len/2; i++){
    let temp=arr[i];
    arr[i]=arr[len-i-1];
    arr[len-i-1]=temp;
}
for(let i=0; i<len; i++){
    console.log(arr[i]);
}


>> printing by using for-of loop

let arr=['apple','mango','banana','orange'];
for(let i of arr){
    console.log(i);
}
