// by using for loop

let price=[250,645,300,900,50];
let percent=0;
let sum=0;
for(let i=0; i<price.length; i++){
    percent=0.1*price[i];
    sum=price[i]-percent;
    price[i]=sum;
}

for(let i=0; i<price.length; i++){
    console.log(price[i]);
}


// using for-of loop

let price=[250,645,300,900,50];
let percent=0;
let sum=0;
for(let i of price){
    percent=0.1*i;
    sum=i-percent;
    i=sum;
    console.log(i);
}
